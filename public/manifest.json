{
  "manifest_version": 2,
  "name": "dmarktab-new",
  "version": "1.2.3",
  "description": "Панель закладок с независимой навигацией по папкам",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "bookmarks",
    "storage",
    "contextMenus",
    "<all_urls>",
    "tabs",
    "sessions"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "bookmarks-toolbar@example.com",
      "strict_min_version": "74.0"
    }
  },
  "browser_action": {
    "default_title": "Закладки"
  }
}